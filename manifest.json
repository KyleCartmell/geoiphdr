{
  "manifest_version": 2,

  "name": "geoiphdr",
  "description": "customizable HTTP header IP address injection",
  "version": "1.0.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background" : {
    "scripts": [
      "background.js"
    ]
  }
}
